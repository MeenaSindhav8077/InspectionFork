@model  Inspection.Web.Models.mAINPROGRESSModel
@using Inspection.Web.Service
@{
    ViewBag.Title = "Index";
}

<div class="row">
    <div class="col-12">
        <div class="page-title-box d-sm-flex align-items-center justify-content-between">
            <h4 class="mb-sm-0">
                <span id="ContentPlaceHolder1_lblaction"></span>&nbsp; Inward Data
            </h4>
        </div>
    </div>
</div>
<div class="card">
    <div class="card-body">
        <div class="live-preview">
            <div class="row gy-4">
                <div class="col-xxl-2 col-md-6">
                    <div>
                        <label>Inward Date</label>
                        @Html.TextBoxFor(model => model._INWARD.InwardDate, "{0:dd, MMM yyyy}", new { @disabled = "disabled", @class = "aspNetDisabled form-control invdate" })
                    </div>
                </div>
                <div class="col-xxl-2 col-md-6">
                    <div>
                        <label>Inward Time</label>
                        @Html.TextBoxFor(model => model._INWARD.InwardTime, new { @disabled = "disabled", @class = "aspNetDisabled form-control" })
                    </div>
                </div>
                <div class="col-xxl-2 col-md-6">
                    <div>
                        <label>Job No</label>
                        @Html.TextBoxFor(model => model._INWARD.JobNo, new { @disabled = "disabled", @class = "aspNetDisabled form-control job-input" })
                    </div>
                </div>
                <div class="col-xxl-2 col-md-6">
                    <div>
                        <label>Part No</label>
                        @Html.TextBoxFor(model => model._INWARD.Partno, new { @disabled = "disabled", @class = "aspNetDisabled form-control" })
                    </div>
                </div>
                <div class="col-xxl-2 col-md-6">
                    <div>
                        <label>Inspection Type</label>
                        @Html.TextBoxFor(model => model._INWARD.InspectionType, new { @disabled = "disabled", @class = "aspNetDisabled form-control" })
                    </div>
                </div>
                <div class="col-xxl-1 col-md-6">
                    <div>
                        <label>Epicor Rev</label>
                        @Html.TextBoxFor(model => model._INWARD.ERev, new { @disabled = "disabled", @class = "aspNetDisabled form-control" })
                    </div>
                </div>
                <div class="col-xxl-1 col-md-6">
                    <div>
                        <label>Actual Rev</label>
                        @Html.TextBoxFor(model => model._INWARD.ActualRev, new { @disabled = "disabled", @class = "aspNetDisabled form-control" })
                    </div>
                </div>
                <div class="col-xxl-2 col-md-6">
                    <div>
                        <label>Qty</label>
                        @Html.TextBoxFor(model => model._INWARD.Qty, new { @disabled = "disabled", @class = "aspNetDisabled form-control", @id = "mainqty" })
                    </div>
                </div>
                <div class="col-xxl-2 col-md-6">
                    <div>
                        <label>Stage</label>
                        @Html.TextBoxFor(model => model._INWARD.ProcessStage, new { @disabled = "disabled", @class = "aspNetDisabled form-control" })
                    </div>
                </div>
            </div>
        </div>
    </div>
    <br>
</div>
<br>
<div class="row">
    <div class="col-12">
        @if (Model != null)
        {
            var ins = " Inspection";
