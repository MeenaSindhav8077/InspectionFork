@{
    int Count = 1;
    var inspectionservice = new Inspection.Web.Scripts.Inspectionservice();
    var inspectiontype = TempData.Peek("inspectiontype")?.ToString();
    var insvalue = Session["inspection_type"] != null ? Session["inspection_type"].ToString() : "";
}

@if (!string.IsNullOrEmpty(inspectiontype) || !string.IsNullOrEmpty(insvalue))
{
    string current_type = inspectiontype;
    int n = 0;
    string _qty = Model._INWARD.acceptqty.ToString();
    if ((current_type == "Final" && (Model._INWARD.finalinspection <= Model.TOTALfinalQTY)) && (Model.finalstatus == false || Model.finalstatus == null) && Model.finalruning != true)
    {
        n = 1;
    }
    else if ((current_type == "Thread" && Model._INWARD.threadinspection == Model.TOTALtharedQTY) && (Model.tharedstatus == false || Model.tharedstatus == null))
    {
        n = 2;
    }
    else if ((current_type == "Visual" && Model._INWARD.visualinspection == Model.TOTALvisualQTY) && (Model.visualstatus == false || Model.visualstatus == null))
    {
        n = 3;
    }
    else if ((current_type == "Humidity" && Model._INWARD.humidity == Model.TOTALhumidityQTY) && (Model.humiditystatus == false || Model.humiditystatus == null))
    {
        n = 4;
    }
    bool Visibility = (n > 0);

    if (Visibility)
    {
        <div class="modal fade  show" id="showModalhhh" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
            <div class="modal-dialog modal-dialog-centered">
                <div class="modal-content">
                    <div class="modal-header bg-light p-3">
                        <h5 class="modal-title" id="exampleModalLabel">@inspectiontype Inspection</h5>
                        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"
                                id="close-modal"></button>
                    </div>
                    @using (Html.BeginForm("statuschange", "visualinspection", FormMethod.Post))
                    {
                        Model._INWARD.InspectionType = inspectiontype;
                        @Html.HiddenFor(model => Model._INWARD.id)
                        @Html.HiddenFor(model => Model._INWARD.Qty)
                        @Html.HiddenFor(model => Model._INWARD.JobNo)
                        @Html.HiddenFor(model => Model._INWARD.Partno)
                        @Html.HiddenFor(model => Model._INWARD.ProcessStage)
                        @Html.HiddenFor(model => Model._INWARD.InspectionType)
                        @Html.HiddenFor(model => Model._INWARD.QualityStage)
                <div class="modal-body">
                    <div class="mb-3">
                        @Html.LabelFor(model => model._submodel.Stage, new { @class = "form-label" })
                        @{
                            var STAGESelectList = inspectionservice.GetInspectiontypewise(inspectiontype);
                        }

                        @Html.DropDownListFor(model => model._submodel.Stage, new SelectList(STAGESelectList, "Value", "Text"), "Select", new { @class = "form-select ", id = "" })
                        @Html.ValidationMessageFor(model => model._submodel.Stage, "", new { @class = "text-danger" })
                    </div>
                    <div class="mb-3">
                        @Html.LabelFor(model => model._submodel.DecisionQty, new { @class = "form-label" })
                        @Html.TextBoxFor(model => model._submodel.DecisionQty, new { @class = "form-select ", id = "" })
                        @Html.ValidationMessageFor(model => model._submodel.DecisionQty, "", new { @class = "text-danger" })
                    </div>
                </div>
                    <div class="modal-footer">
                        <div class="hstack gap-2 justify-content-end">
                            <button type="button" class="btn btn-light" data-bs-dismiss="modal">Close</button>
                            <button type="submit" class="btn btn-success">Save</button>
                        </div>
                    </div>
                    }
                </div>
            </div>
        </div>
    }
}

