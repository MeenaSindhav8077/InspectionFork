                if (humidity != null) {
                    quantities.push(parseFloat(humidity.value) || 0);
                }
            }
        }

        quantities.forEach(num => {
            sum += num;
        })

        console.log(quantities);

        if (sum <= MSqty) {

            $.ajax({
                url: '/visualinspection/ENDTIMEQTY?id=' + id + '&endtime=' + endt + '&qty=' + qty + '&instype=' + inspectiontype,
                type: 'POST',
                success: function (response) {
                    location.reload();
                },
                error: function (error) {

                    console.error(error);
                }
            });
        }
        else {
            alert("Enter valid Quantity")
        }
    }

</script>
