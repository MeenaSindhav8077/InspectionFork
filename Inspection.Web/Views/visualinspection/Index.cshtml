                buttonsStyling: false,
                showCloseButton: true
            })

        }
    });

</script>

<script>
    function  getendidqty(id, typevalue) {
        debugger
        var inspectiontype = typevalue;

        var _id = $('#mainid').val();

        var endt = "";
        var qty = "";
        var MSqty = "";
        var quantities = [];
        let sum = 0;

        if (typevalue == "Final") {
            debugger
            var finaltable = document.getElementById("FinalTables");
            endt = $('#Finalendtime').val();
            qty = $('#FinalQty').val();
            MSqty = $('#smainqtyFinal').val();

            sum = parseInt(qty);

            for (var i = 1; i < finaltable.rows.length; i++) {

                var finals = finaltable.rows[i].querySelector('input[id="insqttyFinal"]');
                if (finals != null) {
                    quantities.push(parseFloat(finals.value) || 0);
                }
            }

        }
        else if (typevalue == "Visual") {
            var Visualtable = document.getElementById("VisualTables");
            MSqty = $('#smainqtyVisual').val();
            endt = $('#Visualendtime').val();
            qty = $('#Visualqty').val();
            sum = parseInt(qty);
            for (var i = 1; i < Visualtable.rows.length; i++) {

                var visual = Visualtable.rows[i].querySelector('input[id="insqttyVisual"]');
                if (visual != null) {
                    quantities.push(parseFloat(visual.value) || 0);
                }
            }
        }
        else if (typevalue == "Thread") {
            var Threadtable = document.getElementById("ThreadTables");
            MSqty = $('#smainqtyThread').val();
            endt = $('#Threadenddate').val();
            qty = $('#Threadqty').val();
            sum = parseInt(qty);
            for (var i = 1; i < Threadtable.rows.length; i++) {

                var thared = Threadtable.rows[i].querySelector('input[id="insqttyThread"]');
                if (thared != null) {
                    quantities.push(parseFloat(thared.value) || 0);
                }
            }
        }
        else if (typevalue == "Humidity") {
            var Humiditytable = document.getElementById("HumidityTables");
            MSqty = $('#smainqtyHumidity').val();
            endt = $('#Humidityendtime').val();
            qty = $('#Humidityqty').val();
